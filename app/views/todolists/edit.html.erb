<h1>編集</h1>
<%= form_with model:@list,url: todolist_path(@list.id),local:true do |f| %>
<div>
    <lable>画像</lable>
    <%= f.attachment_field:image %>
</div>
<div>
    <label>タイトル</label>
    <%= f.text_field:title %>
</div>
<div>
    <label>本文</label>
    <%= f.text_area:body %>
</div>
<div>
    <%= f.submit "更新" %>
</div>
<% end %>